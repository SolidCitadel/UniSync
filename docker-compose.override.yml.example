# docker-compose.override.yml.example
#
# 용도:
# - 로컬 개발 시 개인화 설정 (포트 변경, 추가 환경변수 등)
# - docker-compose up 실행 시 자동으로 적용됨
#
# 사용법:
# 1. cp docker-compose.override.yml.example docker-compose.override.yml
# 2. 필요한 부분만 수정
#
# 참고:
# - docker-compose.override.yml은 .gitignore에 등록됨
# - 이 파일은 템플릿으로 커밋됨

services:
  # MySQL 포트 변경 예시
  mysql:
    ports:
      - "3308:3306"  # 기본 3307 대신 3308 사용

  # LocalStack 추가 서비스 활성화 예시
  localstack:
    environment:
      - DEBUG=1
      - SERVICES=sqs,stepfunctions,lambda,s3,secretsmanager,events,cognito-idp,elasticloadbalancingv2,ec2,dynamodb

  # 추가 개인화 설정이 필요하면 여기에 작성
